{
  "name": "pew-git",
  "tree": {
    "$className": "DataModel",

    "Players":{
      "$className": "Players",
      "$properties": {
        "CharacterAutoLoads": false
      }
    },

    "ReplicatedStorage": {
      "$path": "src/shared",
      "$className": "ReplicatedStorage",
      "Packages": {
          "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/server"
    },

    "StarterPlayer": {
      "$properties": {
        "LoadCharacterAppearance": false,
        "UserEmotesEnabled": false,
        "CharacterJumpPower": 50,

        "DevComputerMovementMode": "Scriptable",
        "DevTouchMovementMode": "Scriptable",

        "EnableMouseLockOption": false
      },
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "src/client"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 0,
        "GlobalShadows": false,
        "Outlines": false,
        "Technology": "Compatibility"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}